{
  "name": "nemassler",
  "author": "诡锋 Vincent",
  "version": "1.0.3",
  "description": "Transform .ncm audio format into .mp3 format, and get beats per minute(BPM) of MP3s",
  "main": "main.js",
  "config": {
    "forge": "./forge.config.js"
  },
  "scripts": {
    "serve": "nodemon app.js",
    "page": "node app.js",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "frontend": "cd frontend && npm run serve",
    "frontend-build": "cd frontend && npm run build",
    "build-app": "npm run frontend-build && npm run make"
  },
  "license": "ISC",
  "dependencies": {
    "aes-js": "^3.1.2",
    "axios": "^1.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-dmg": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "electron": "^26.2.1",
    "nodemon": "^3.0.1"
  }
}
