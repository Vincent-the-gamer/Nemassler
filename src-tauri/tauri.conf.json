{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8080"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "nemassler",
  "mainBinaryName": "nemassler",
  "version": "2.0.0",
  "identifier": "com.vincent.gamer",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "center": true,
        "title": "nemassler",
        "width": 1280,
        "height": 720,
        "minWidth": 820,
        "minHeight": 400,
        "decorations": false
      }
    ]
  }
}